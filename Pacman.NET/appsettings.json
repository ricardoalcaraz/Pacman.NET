{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "WebAppShared": "Debug",
      "ModelsShared": "Debug"
    }
  },
  "AllowedHosts": "*",
  "Pacman": {
    "Configuration": "/etc/pacman.conf",
    "DbDirectory": "/var/lib/pacman/sync",
    "Cache": "/var/cache/pacman/pkg",
    "CustomRepos": [
      {
        "Name": "",
        "SourceDir": ""
      }
    ]
  },
  "Pacman.NET": {
    "CachePackages": true,
    "CacheDb": true,
    "CacheRefreshInterval": 2,
    "LogDirectory": "/var/log/Pacman.NET",
    "MirrorUrl": "https://archlinux.org/mirrors/status/json/",
    "VerifySignature": true
  }
}
